[user]
	name = i05nagai
	email = i05nagai@gmail.com
[core]
	editor = vim
[alias]
	l = log --abbrev-commit --date=iso --graph --pretty=format:'%C(red)%h %C(yellow)%d%C(green)%cd %C(blue)%cn\n%C(reset)%s\n'
	ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
	pom = push origin master
	po = push origin
	s = status
	initial-commit = commit --allow-empty -m 'Initial commit'
	alias = !git config --get-regexp alias
	make-patch = diff --no-prefix
	rebase-fixup = rebase --autosquash -i
	find = "!git ls-files | grep -i"
	local-me = !git config --local user.name "i05nagai" && git config --local user.email "i05nagai@gmail.com"
[color]
	ui = auto
